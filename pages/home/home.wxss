.margin-top-10 {
  margin-top: 10px;
}

.searchbar-result {
  margin-top: 0;
  font-size: 14px;
}

.searchbar-result:before {
  display: none;
}

.weui-cell {
  padding: 12px 15px 12px 35px;
}

.videoHome {
  width: 100%;
}

.imageProjects {
